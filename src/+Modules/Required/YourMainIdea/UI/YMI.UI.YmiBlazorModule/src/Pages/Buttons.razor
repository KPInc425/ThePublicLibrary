@page "/ymi/buttons"

<PageTitle>Buttons</PageTitle>

<MudContainer>
    <MudStack Class="mt-5">
        <MudContainer>
            <MudButton Variant="Variant.Filled" Disabled="true">Default</MudButton>
            <MudButton Color="Color.Primary" Variant="Variant.Filled">Primary</MudButton>
            <MudButton Color="Color.Secondary" Variant="Variant.Text">Secondary</MudButton>
            <MudButton Color="Color.Success" Variant="Variant.Filled" DisableElevation="true">Success</MudButton>
            <MudButton Color="Color.Dark" Variant="Variant.Filled" FullWidth="true">Dark</MudButton>
            @* <MudButton Color="Color.Link">Link</MudButton> *@
        </MudContainer>
        <MudButton Color="Color.Info" Variant="Variant.Outlined">Info</MudButton>
        <MudButton Color="Color.Warning" Variant="Variant.Outlined">Warning</MudButton>
        <MudButton Color="Color.Error" Variant="Variant.Filled">Error</MudButton>
    </MudStack>
</MudContainer>